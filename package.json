{
  "name": "lsp-log-parser",
  "version": "1.0.0",
  "description": "Parser for LSP server logs",
  "author": "Rafał Chłodnicki",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vue/composition-api": "^0.6.3",
    "nuxt": "^2.0.0",
    "vue-json-tree": "^0.4.3",
    "vue-shortkey": "^3.1.7"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.2.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "stylelint": "^13.6.0"
  }
}
